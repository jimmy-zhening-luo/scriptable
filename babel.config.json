{
  "$schema": "https://json.schemastore.org/babelrc",
  "minified": true,
  "comments": false,
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": true
        }
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "allowDeclareFields": true,
        "optimizeConstEnums": true,
        "rewriteImportExtensions": true
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}
